 retrieve="SELECT
	CAST('' AS CHAR(1)) AS FLAG,
	CAST(COALESCE(CV.IDPRODUTO, 0) AS CHAR(9)) AS IDPRODUTOE,
	CV.IDMOVIMENTO,
	CV.IDUSUARIO,
	CV.IDPRODUTO,
	CV.IDCLIFOR,
	CV.IDEMPRESA,
	CV.DATA_MOVIMENTO,
	CV.QUANTIDADE,
	CV.TIPOMOVIMENTO,
	CV.PRECO,
	P.NOME,
	CE.QUANTIDADE
FROM
	COMPRA_VENDA CV
	LEFT JOIN PRODUTO P ON(
	P.IDPRODUTO = CV.IDPRODUTO)
	LEFT JOIN CONTROLE_ESTOQUE CE ON(
	CE.IDPRODUTO = CV.IDPRODUTO AND
	CE.IDEMPRESA = CV.IDEMPRESA)
WHERE
	1 = 1" update="COMPRA_VENDA" updatewhere=1 updatekeyinplace=yes  sort="idproduto A " )
